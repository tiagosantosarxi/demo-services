<odoo>
    <record id="product_category_form_view_inherit_vendus" model="ir.ui.view">
        <field name="name">product.category.form.view.inherit.vendus</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='parent_id']" position="after">
                <field name="vendus_category_id" class="oe_inline"/>
            </xpath>
        </field>
    </record>
</odoo>