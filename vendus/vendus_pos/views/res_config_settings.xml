<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.vendus_account</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vendus_test']" position="after">
                <label class="col-lg-3 o_light_label" string="Vendus Account URL" for="vendus_url"/>
                <field name="vendus_url"/>
            </xpath>
        </field>
    </record>
</odoo>
